<UserControl x:Class="_78MultipleScrollViewer.UserControls.HamburgerMenuVIews.AllOwned"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_78MultipleScrollViewer.UserControls.HamburgerMenuVIews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <ScrollViewer x:Name="MainScrollViewer" HorizontalScrollBarVisibility="Hidden">
        <StackPanel>
            <local:HamburgerMenuHeader x:Name="HamHeader" />
            <local:HamburgerMenuAppList x:Name="HamAppsList" />
        </StackPanel>
    </ScrollViewer>
</UserControl>
